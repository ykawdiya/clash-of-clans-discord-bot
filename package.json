{
  "name": "clash-of-clans-discord-bot",
  "version": "1.0.1",
  "description": "A Discord bot for Clash of Clans clan management and statistics",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit",
    "test:debug": "jest --detectOpenHandles",
    "build": "tsc",
    "start:ts": "node dist/index.js"
  },
  "keywords": [
    "discord",
    "bot",
    "clash-of-clans",
    "coc"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "https-proxy-agent": "^5.0.1",
    "mongodb": "^6.15.0",
    "mongoose": "^7.0.3",
    "node-cache": "^5.1.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.13.14",
    "eslint": "^8.37.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
